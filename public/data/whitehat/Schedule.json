
    {
        "name": "Schedule API",
        "methods": [
            {
                "MethodName": "List schedules",
                "Synopsis": "List all schedules owned by user.",
                "HTTPMethod": "GET",
                "URI": "/schedule",
                "RequiresOAuth": "N",
                "parameters": [
                    {
                        "Name": "name",
                        "Required": "N",
                        "Type": "string",
                        "Default":"",
                        "Description": "Scan name."
                    },
                    {
                        "Name": "start_date",
                        "Required": "N",
                        "Type": "string",
                        "Default":"",
                        "Description": "Start date."
                    },
                    {
                        "Name": "end_date",
                        "Required": "N",
                        "Type": "string",
                        "Default":"",
                        "Description": "End date."
                    },
                    {
                        "Name": "duration",
                        "Required": "N",
                        "Type": "string",
                        "Default":"",
                        "Description": "Time duration in seconds."
                    },
                    {
                        "Name": "timezone",
                        "Required": "N",
                        "Type": "string",
                        "Default":"",
                        "Description": "Time zone of schedule. Ex: America/New_York"
                    },
                    {
                        "Name": "job",
                        "Required": "N",
                        "Type": "integer",
                        "Default":"",
                        "Description": "Job id"
                    },
                    {
                        "Name": "creator",
                        "Required": "N",
                        "Type": "integer",
                        "Default":"",
                        "Description": "Job schedule creator."
                    },
                    {
                        "Name": "accept_fmt",
                        "Required": "N",
                        "Type": "enumerated",
                        "EnumeratedList": ["application/json", "text/xml"],
                        "Description": "Format of results."
                    },
                    {
                        "Name": "page:limit",
                        "Required": "N",
                        "Default": "",
                        "Type": "integer",
                        "Description": "Number of pages to retrieve."
                    },
                    {
                        "Name": "page:offset",
                        "Required": "N",
                        "Default": "",
                        "Type": "integer",
                        "Description": "Begin retrieving pages from this index."
                    }
                ]
            },
            {
                "MethodName": "Get single schedule",
                "Synopsis": "List a single schedule.",
                "HTTPMethod": "GET",
                "URI": "/schedule/:id",
                "RequiresOAuth": "N",
                "parameters": [
                    {
                        "Name": "id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "Schedule id; enter one schedule id only, putting more than one (or none) will result in all schedules being displayed."
                    },
                    {
                        "Name": "accept_fmt",
                        "Required": "N",
                        "Type": "enumerated",
                        "EnumeratedList": ["application/json", "text/xml"],
                        "Description": "Format of results."
                    }
                ]
            },
            {
                "MethodName": "Delete a schedule",
                "Synopsis": "Delete a single schedule.",
                "HTTPMethod": "DELETE",
                "URI": "/schedule/:id",
                "RequiresOAuth": "N",
                "parameters": [
                    {
                        "Name": "id",
                        "Required": "Y",
                        "Type": "integer",
                        "Description": "Schedule id"
                    }
                ]
            }
        ]
    }
